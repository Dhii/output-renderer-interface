{
    "name": "dhii/output-renderer-interface",
    "description": "An interface for objects that can perform rendering.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "dhii/stringable-interface": "^0.1"
    },
    "require-dev": {
        "dhii/php-cs-fixer-config": "dev-php-5.3",
        "phpunit/phpunit": "^6.0",
        "codeclimate/php-test-reporter": "dev-master",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Output\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\Output\\UnitTest\\": "test/unit",
            "Dhii\\Output\\FuncTest\\": "test/functional",
            "Dhii\\Output\\Test\\": "test/stubs"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev",
            "0.4.x": "0.4.x-dev"
        }
    },
    "suggest": {
        "psr/container": "To use PSR-11 containers as context"
    }
}
